<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="c7c7ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c7c7ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodoli\b.*">
      <option name="FOREGROUND" value="c7c7ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <filter name="GC7">
      <pattern index="2" />
    </filter>
  </component>
</application>